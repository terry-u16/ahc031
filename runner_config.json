{
  "ProblemOption": {
    "ProblemName": "ahc031",
    "TimeLimit": "00:00:05"
  },
  "RunnerOption": {
    "StartSeed": 0,
    "EndSeed": 300,
    "ReferenceScore": 1000000,
    "LocalParallelCount": 30,
    "CloudParallelCount": 300,
    "SummaryFilePath": "./data/score_history.txt",
    "ResultDirectoryPath": "./data/results"
  },
  "ExecutionOption": {
    "ScoreRegex": "^Score = (?<score>\\d+)$",
    "SeedFormat": "0000",
    "Timeout": "00:00:30",
    "LocalExecutionSteps": [
      {
        "ExecutionCommand": "ahc031",
        "StdInPath": "data/in/{SEED}.txt",
        "StdOutPath": "data/out/{SEED}.txt",
        "StdErrorPath": "data/err/{SEED}.txt"
      },
      {
        "ExecutionCommand": "vis data/in/{SEED}.txt data/out/{SEED}.txt",
        "StdInPath": null,
        "StdOutPath": null,
        "StdErrorPath": null
      }
    ],
    "CloudExecutionSteps": [
      {
        "ExecutionCommand": "ahc031",
        "StdInPath": "data/in/{SEED}.txt",
        "StdOutPath": "data/out/{SEED}.txt",
        "StdErrorPath": "data/err/{SEED}.txt"
      },
      {
        "ExecutionCommand": "vis data/in/{SEED}.txt data/out/{SEED}.txt",
        "StdInPath": null,
        "StdOutPath": null,
        "StdErrorPath": null
      }
    ],
    "Files": [
      "main",
      "vis",
      "in/{SEED}.txt"
    ]
  },
  "CompileOption": {
    "ExeName": "main",
    "Files": [
      {
        "Source": "expanded.rs",
        "Destination": "src/bin/main.rs"
      }
    ]
  },
  "GoogleCloudOptions": {
    "RunEndpoint": "null",
    "RustCompilerEndpoint": "null",
    "BucketName": "null"
  },
  "BlobClientOption": {
    "ConnectionString": "null"
  }
}